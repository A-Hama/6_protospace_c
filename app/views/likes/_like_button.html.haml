.like-prototype{id: "like-#{prototype.id}"}
  - if user_signed_in?
    - if prototype.like_user(current_user.id)
      = link_to prototype_like_path(id: prototype.likes.ids, prototype_id: prototype.id), method: :delete, remote: true do
        .fa.fa-heart.fa-2x.heart-red{id: "heart-#{prototype.id}", style: 'color: red;'}
          = prototype.likes.count.to_s
    - else
      = link_to prototype_likes_path(prototype_id: prototype.id), method: :post, remote: true do
        .fa.fa-heart-o.fa-2x.heart-color{id: "heart-#{prototype.id}"}
          = prototype.likes.count.to_s

